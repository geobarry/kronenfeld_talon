os: windows
and app.name: Inkscape vector graphics editor
os: windows
and app.exe: inkscape.exe
-
#common shortcuts
zoom in:
	key(+)
zoom out:
	key(-)
pan left:
	key(ctrl-right)
pan right:
	key(ctrl-left)
pan up:
	key(ctrl-down)
pan down:
	key(ctrl-up)
select tool:
	key(s)
nudge right:
	key(alt-right)
nudge left:	
	key(alt-left)
nudge up:
	key(alt-up)
nudge down:
	key(alt-down)